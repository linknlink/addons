{
  "name": "Network Manager",
  "version": "0.0.1",
  "slug": "network_manager",
  "description": "为 Ubuntu Server 系统（特别是鲁班猫设备）提供 WiFi 网络管理功能的 Docker 容器应用",
  "arch": ["aarch64", "amd64", "armv7"],
  "startup": "services",
  "boot": "auto",
  "options": {
    "wifi_scan_interval": 30,
    "auto_reconnect": true,
    "default_ip_method": "dhcp",
    "log_level": "info",
    "initial_wifi_ssid": "",
    "initial_wifi_password": "",
    "initial_wifi_ip_address": "",
    "initial_wifi_gateway": "",
    "initial_wifi_dns": ""
  },
  "schema": {
    "wifi_scan_interval": "int",
    "auto_reconnect": "bool",
    "default_ip_method": "str",
    "log_level": "str",
    "initial_wifi_ssid": "str",
    "initial_wifi_password": "str",
    "initial_wifi_ip_address": "str",
    "initial_wifi_gateway": "str",
    "initial_wifi_dns": "str"
  }
}
